{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended", "next", "next/core-web-vitals", "prettier", "plugin:@typescript-eslint/recommended"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "@typescript-eslint"
  ],
  "rules": {
      "react/react-in-jsx-scope": "off",
      "indent": "off",
      "camelcase": 0,
      "react/jsx-indent": 0,
      "no-return-assign": 0,
      "jsx-a11y/label-has-associated-control": 0,
      "no-unused-expressions": 0,
      "no-nested-ternary": 0,
      "prettier/prettier": ["error", { "endOfLine": "auto" }],
      "no-shadow": 0,
      "no-param-reassign": 0,
      "consistent-return": 0,
      "array-callback-return": 0,
      "no-useless-catch": 0,
      "no-case-declarations": 0,
      "no-async-promise-executor": 0,
      "react/jsx-curly-newline": 0,
      "react/jsx-wrap-multilines": 0,
      "react/jsx-props-no-spreading": 0,
      "react/no-array-index-key": 0,
      "react/display-name": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/no-unknown-property": [2, { "ignore": ["tw", "css", "jsx", "html"] }],
      "import/prefer-default-export": "off",
      "react-hooks/exhaustive-deps": "off",
      "react/prop-types": "off",
      "no-use-before-define": "off",
      "no-else-return": "off",
      "@typescript-eslint/no-explicit-any": "off"
  }
}